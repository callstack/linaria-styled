{
  "name": "linaria-styled",
  "version": "0.0.0",
  "private": true,
  "description": "Experimental Babel plugin to extract CSS statically",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "babel.js",
    "loader.js"
  ],
  "repository": "git@github.com:callstack/linaria-styled.git",
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel-plugin",
    "babel",
    "module",
    "require",
    "import"
  ],
  "scripts": {
    "prepare": "babel src --out-dir lib --ignore '**/__tests__/**,**/__integration-tests__/**,**/__fixtures__/**' --source-maps && flow-copy-source -i '{**/__tests__/**,**/__integration-tests__/**,**/__fixtures__/**}' src lib",
    "lint": "eslint .",
    "flow": "flow",
    "test": "jest",
    "test:integration:ci": "jest __integration-tests__"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "codecov": "^3.1.0",
    "eslint": "^5.4.0",
    "eslint-config-satya164": "^2.0.1",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "puppeteer": "^1.8.0",
    "react": "^16.5.1",
    "source-map": "^0.7.3"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/register": "^7.0.0",
    "dedent": "^0.7.0",
    "loader-utils": "^1.1.0",
    "stylis": "^3.5.3"
  },
  "resolutions": {
    "**/babel-core": "7.0.0-bridge.0"
  }
}
